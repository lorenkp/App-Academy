<!DOCTYPE html>
<html>
<head>
  <title>NinetyNineCats</title>
  <%= javascript_include_tag 'application' %>
  <%= stylesheet_link_tag "application" , media: "all" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if flash[:errors] %>
    <div class="errors">
      <span class="errors">
        <%= flash[:errors].join(". ") %>
      </span>
    </div>
  <% end %>

  <%= yield %>

</body>
</html>
